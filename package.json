{
    "author": "focus@kleegroup.com",
    "name": "ffs-zera",
    "description": "Focus demonstration application",
    "version": "0.0.0",
    "homepage": "https://github.com/KleeGroup/focus-demo-app",
    "repository": {
        "type": "git",
        "url": "https://github.com/KleeGroup/focus-demo-app.git"
    },
    "engines": {
        "node": "6.9.x",
        "npm": "3.10.x"
    },
    "scripts": {
        "production": "better-npm-run production",
        "recette": "better-npm-run recette",
        "analyze": "better-npm-run analyze",
        "build": "npm run production",
        "start": "better-npm-run webpack-dev-server-npm-focus",
        "watch": "better-npm-run watch",
        "patch": "cp ./patch/built-in-components.js ./node_modules/focus-components/list/mixin && cp ./patch/menu.js ./node_modules/focus-components/components/menu/index.js",
        "postinstall": "npm run patch"
    },
    "main": "app/index.js",
    "betterScripts": {
        "production": {
            "command": "webpack --progress",
            "env": {
                "ANCHOR_CLASS": "demo-app",
                "BABELIFIED_PATH": "./app",
                "ENTRY_FILE_PATH": "./app/index",
                "PAGE_TITLE": "Fight For Sub",
                "OUTPUT_DIR": "./docs",
                "OUTPUT_PUBLIC_PATH": "/",
                "BASE_URL": "/",
                "API_ROOT": "https://ffs-api.zerator.com/v1/",
                "DEV": "false",
                "SOURCE_MAPS": "false",
                "MINIMIFY": "true",
                "DROP_CONSOLE": "true",
                "NODE_ENV": "production",
                "ROOT_URL": "https://ffs.zerator.com/",
                "CLIENT_ID": "uzjude5lwse23lg2aobz2m2ywa5vb1"
            }
        },
        "recette": {
            "command": "webpack --progress",
            "env": {
                "ANCHOR_CLASS": "demo-app",
                "BABELIFIED_PATH": "./app",
                "ENTRY_FILE_PATH": "./app/index",
                "PAGE_TITLE": "Fight For Sub",
                "OUTPUT_DIR": "./docs",
                "OUTPUT_PUBLIC_PATH": "/ffs-zera/",
                "BASE_URL": "/",
                "API_ROOT": "https://ffs-api-test.zerator.com/v1/",
                "DEV": "false",
                "SOURCE_MAPS": "false",
                "MINIMIFY": "true",
                "DROP_CONSOLE": "true",
                "NODE_ENV": "production",
                "ROOT_URL": "https://hartorn.github.io/ffs-zera",
                "CLIENT_ID": "c8ax3vskq7wdxsxf1t4whhl9c3f9bd"
            }
        },
        "analyze": {
            "command": "webpack --progress",
            "env": {
                "ANCHOR_CLASS": "demo-app",
                "BABELIFIED_PATH": "./app",
                "ENTRY_FILE_PATH": "./app/index",
                "PAGE_TITLE": "Fight For Sub",
                "OUTPUT_DIR": "./prod",
                "OUTPUT_PUBLIC_PATH": "./",
                "API_ROOT": "/",
                "BASE_URL": "/",
                "DEV": "false",
                "SOURCE_MAPS": "false",
                "MINIMIFY": "true",
                "DROP_CONSOLE": "true",
                "NODE_ENV": "production",
                "ANALYZE": "true"
            }
        },
        "watch": {
            "command": "webpack --watch",
            "env": {
                "ANCHOR_CLASS": "demo-app",
                "BABELIFIED_PATH": "./app",
                "ENTRY_FILE_PATH": "./app/index",
                "PAGE_TITLE": "Focus demonstration application",
                "OUTPUT_DIR": "./watch",
                "OUTPUT_PUBLIC_PATH": "./",
                "API_ROOT": "/",
                "BASE_URL": "/",
                "NODE_ENV": "development"
            }
        },
        "webpack-dev-server-npm-focus": {
            "command": "focus-dev-server",
            "env": {
                "ANCHOR_CLASS": "demo-app",
                "BABELIFIED_PATH": "./app",
                "ENTRY_FILE_PATH": "./app/index",
                "PAGE_TITLE": "Focus demonstration application",
                "OUTPUT_DIR": "./public",
                "OUTPUT_PUBLIC_PATH": "./",
                "API_ROOT": "https://ffs-api-test.zerator.com/v1/",
                "BASE_URL": "/",
                "ROOT_URL": "http://localhost:3000/",
                "CLIENT_ID": "cs5900eog2wajva8rnvf2jtx6gazsvd",
                "NODE_ENV": "development",
                "HOT_RELOAD": "true",
                "GENERATE_HTML": "true",
                "USE_CACHE": "false",
                "PARALLEL_BUILD": "true",
                "DEV": "true"
            }
        }
    },
    "license": "MIT",
    "devDependencies": {
        "better-npm-run": "0.1.0",
        "eslint-config-focus": "0.6.0",
        "eslint-import-resolver-webpack": "0.8.3",
        "focus-components": "2.2.0",
        "focus-core": "2.2.0",
        "focus-devtools": "0.4.1",
        "font-awesome": "4.7.0",
        "jquery": "3.2.1",
        "localforage": "1.5.3",
        "lodash": "4.17.4",
        "material-design-icons-iconfont": "3.0.3",
        "material-design-lite": "1.3.0",
        "moment": "2.19.1",
        "normalize.css": "7.0.0",
        "numeral": "2.0.6",
        "react": "15.4.2",
        "react-addons-css-transition-group": "15.4.2",
        "react-dom": "15.4.2",
        "react-fontawesome": "1.6.1",
        "react-router": "3.0.5",
        "typeface-roboto": "0.0.45",
        "webpack-focus": "1.0.0"
    }
}